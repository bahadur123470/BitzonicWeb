{
  "version": 2,
  "name": "bitzone-web",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "pages/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "headers": { "cache-control": "max-age=31536000, immutable" } },
    { "src": "/Js/(.*)", "headers": { "cache-control": "max-age=31536000, immutable" } },
    { "src": "/pages/(.*)", "dest": "/pages/$1.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
